{
  "name": "app-express",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:vianaz/app-express.git",
  "author": "Gabriel Viana Boaes <viana_gab2001@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node build/server.js",
    "build": "babel src --extensions '.js,.ts' --out-dir build --no-copy-ignored",
    "dev": "tsnd --transpile-only --respawn --ignore-watch node_modules --exit-child --cls --rs -r dotenv/config -r tsconfig-paths/register src/server.ts",
    "prisma deploy": "dotenv -e .env.production prisma migrate deploy",
    "prisma seed deploy": "dotenv -e .env.production prisma db seed",
    "test": "clear && dotenv -e .env.test jest --i --verbose"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "4.1.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "babel-plugin-module-resolver": "^4.1.0",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.20.0",
    "faker": "^6.6.6",
    "jest": "^28.1.3",
    "prisma": "^4.1.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
